{
  "name": "ember-holy-futuristic-template-namespacing-batman-sigil-change-codemod",
  "version": "1.0.1",
  "keywords": [
    "codemod-cli"
  ],
  "repository": "git@github.com:rwjblue/ember-holy-futuristic-template-namespacing-batman-sigil-change-codemod.git",
  "license": "MIT",
  "author": "Robert Jackson <me@rwjblue.com>",
  "main": "index.js",
  "bin": "./bin/cli.js",
  "scripts": {
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "lint": "eslint --cache .",
    "test": "codemod-cli test",
    "test:coverage": "codemod-cli test --coverage",
    "update-docs": "codemod-cli update-docs"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "codemod-cli": "^2.0.0",
    "ember-template-recast": "^3.1.3"
  },
  "devDependencies": {
    "coveralls": "^3.0.4",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "release-it": "^13.6.6",
    "release-it-lerna-changelog": "^1.0.3"
  },
  "engines": {
    "node": "8.* || 10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "hooks": {
      "after:bump": "yarn update-docs"
    },
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  }
}
